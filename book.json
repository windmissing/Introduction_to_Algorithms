{
    "gitbook": "3.2.3",
    "author": "windmissing",
    "plugins": [
        "katex",
        "katex-plus",
        "alerts",
        "github",
        "github-buttons",
        "splitter",
        "-sharing", "sharing-plus",
        "donate",
        "copy-code-button",
        "anchor-navigation-ex",
        "sectionx",
        "click-reveal"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/windmissing/introduction-to-algorithms"
        },
        "github-buttons": {
            "repo": "windmissing/introduction-to-algorithms",
            "types": [
                "star",
                "watch",
                "fork"
            ],
            "size": "small"
        },
        "donate": {
            "wechat": "http://windmissing.github.io/images/2020/weixin.jpg",
            "alipay": "http://windmissing.github.io/images/2020/alipay.jpg",
            "title": "",
            "button": "赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
        },
        "anchor-navigation-ex": {
            "isShowTocTitleIcon": true,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        }
    }
}